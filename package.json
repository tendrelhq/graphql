{
  "name": "@tendrelhq/graphql",
  "version": "0.18.0",
  "module": "schema/index.ts",
  "type": "module",
  "scripts": {
    "check": "tsc --pretty && biome check --write",
    "clean": "rimraf **/__generated__/** **/*.generated.ts node_modules out",
    "compile": "bun build.ts",
    "dev": "bun --inspect ./bin/app.ts",
    "generate": "bunx grats && bun ./scripts/generate-legacy-schema.ts && bun ./scripts/print-schema.ts",
    "postgenerate": "treefmt",
    "explore": "bunx ruru@beta -Pe http://localhost:4000",
    "start": "bun ./bin/app.ts",
    "start+tracing": "bun -r ./lib/instrument.ts ./bin/app.ts",
    "postinstall": "bun generate"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@clerk/types": "^4.6.1",
    "@eddeee888/gcg-typescript-resolver-files": "^0.8.1",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typed-document-node": "^5.0.9",
    "@graphql-codegen/typescript": "^4.0.8",
    "@graphql-codegen/typescript-graphql-files-modules": "^3.0.0",
    "@graphql-codegen/typescript-resolvers": "^4.2.0",
    "@graphql-tools/merge": "^9.0.4",
    "@graphql-tools/mock": "^9.0.4",
    "@graphql-tools/schema": "^10.0.4",
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/negotiator": "^0.6.3",
    "@vtsls/language-server": "^0.2.5",
    "graphql-language-service-cli": "^3.4.1",
    "grats": "^0.0.29",
    "merge-anything": "^6.0.2",
    "prettier": "^3.4.1",
    "rimraf": "^5.0.7",
    "typescript-language-server": "^4.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@aws-sdk/client-s3": "^3.692.0",
    "@aws-sdk/s3-request-presigner": "^3.692.0",
    "@clerk/clerk-sdk-node": "^5.0.12",
    "@clerk/shared": "^2.3.1",
    "@formatjs/intl-localematcher": "^0.5.4",
    "@js-temporal/polyfill": "^0.4.4",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.1",
    "@opentelemetry/instrumentation-dataloader": "^0.10.0",
    "@opentelemetry/instrumentation-express": "^0.40.1",
    "@opentelemetry/instrumentation-graphql": "^0.41.0",
    "@opentelemetry/instrumentation-http": "^0.52.1",
    "@opentelemetry/resource-detector-aws": "^1.5.1",
    "@opentelemetry/sdk-node": "^0.52.1",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "graphql-scalars": "^1.23.0",
    "graphql-tag": "^2.12.6",
    "morgan": "^1.10.0",
    "myzod": "^1.11.0",
    "negotiator": "^0.6.3",
    "pattycake": "^0.0.2",
    "postgres": "^3.4.4",
    "ts-pattern": "^5.5.0"
  }
}
